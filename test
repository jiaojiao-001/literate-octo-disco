package lianliankan.bean;

import javax.swing.Icon;
import javax.swing.ImageIcon;
import javax.swing.JButton;

public class Element {
	private ImageIcon icon;
	private int rowX;
	private int colY;
	private int num;
	
	public Element(int rowX,int colY,int num,ImageIcon icon){
		this.icon = icon;
		this.rowX = rowX;
		this.colY = colY;
		this.num = num;
	}
		
	
	public int getNum(){
		return num;
	}	
	
	public void setNum(int num){
		this.num = num;
	}

	public ImageIcon getIcon() {
		return icon;
	}
	
	public void setNumImg(int num,ImageIcon icon){
		this.num = num;
		this.icon = icon;
	}
	
	public int getRowX() {
		return rowX;
	}

	public int getColY() {
		return colY;
	}

	
}
